{
  "name": "@froggr/track-change-extension",
  "version": "2.0.0",
  "description": "A track change extension for tiptap editor v3. Just like the Office Word Revision.",
  "keywords": [
    "tiptap",
    "tiptap3",
    "trackchange",
    "revision",
    "extension"
  ],
  "license": "MIT",
  "author": "Ray Chen<ray_chenyuncai@163.com>",
  "contributors": [
    "froggr (TipTap v3 port)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/froggr/tiptap-track-change-extension.git"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c",
    "dev": "npm run clean && rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@tiptap/core": "^3.7.2",
    "@tiptap/pm": "^3.7.2",
    "rollup": "^3.17.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.0.0",
    "@tiptap/pm": "^3.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
